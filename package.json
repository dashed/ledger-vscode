{
  "name": "ledger",
  "displayName": "Ledger Syntax",
  "description": "Syntax highlighting for ledger files.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "ledger",
      "aliases": ["Ledger", "ledger"],
      "extensions": [".ledger"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "ledger",
      "scopeName": "source.ledger",
      "path": "./syntaxes/ledger.tmLanguage.json"
    }]
  }
}
